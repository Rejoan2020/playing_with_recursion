#include<bits/stdc++.h>
using namespace std;
using ll = long long;

int find_rec(int *a,int n,int c){
    if(n==-1){
        return -1;
    }

    if(a[n]==c){
        return n+1;
    }

    return find_rec(a,n-1,c);

}

int main(){
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        int num = 5;
        cout<<find_rec(a,n-1,num)<<endl;
    }
}
